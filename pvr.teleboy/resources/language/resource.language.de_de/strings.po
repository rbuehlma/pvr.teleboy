#. Setting to set used stream username
#: pvr.teleboy/resources/settings.xml
msgctxt "#30000"
msgid "Username"
msgstr "Benutzername"

#. Setting to set used stream password
#: pvr.teleboy/resources/settings.xml
msgctxt "#30001"
msgid "Password"
msgstr "Passwort"

#. Boolean setting to use favourite streams only
#: pvr.teleboy/resources/settings.xml
msgctxt "#30002"
msgid "Favourite channels only"
msgstr "Nur favorisierte Kanäle nutzen"

#. Help text to setting #30002
#: pvr.teleboy/resources/settings.xml
msgctxt "#30003"
msgid "Load only channels which are in the favourite list."
msgstr "Lädt nur Kanäle welche in favorisierter Liste enthalten sind."

#. Boolean setting to enable dolby stream usage
#: pvr.teleboy/resources/settings.xml
msgctxt "#30004"
msgid "Enable Dolby"
msgstr "Aktiviere Dolby"

#. Help text to setting #30010
#: pvr.teleboy/resources/settings.xml
msgctxt "#30005"
msgid "Enable support for dolby."
msgstr "Aktiviert die Unterstützung für Dolby Streams."

#. Settings category label
#: pvr.teleboy/resources/settings.xml
msgctxt "#30006"
msgid "Teleboy"
msgstr "Teleboy"

#. Settings group label
#: pvr.teleboy/resources/settings.xml
msgctxt "#30007"
msgid "General"
msgstr "Allgemein"

#. Notification message to show on screen if username or password not set
#: src/TeleBoy.cpp
msgctxt "#30100"
msgid "Missing username or password"
msgstr "Fehlender Benutzername oder Passwort"

#. Notification message to show on screen if login failed
#: src/Session.cpp
msgctxt "#30101"
msgid "Login failed!"
msgstr "Anmeldung fehlgeschlagen!"

#. Notification message to show for inform not supported by used account
msgctxt "#30102"
msgid "Free accounts are not supported!"
msgstr "Freie Konten nicht Verfügbar!"

#. Notification message to show on screen if login rate limit reached
#: src/Session.cpp
msgctxt "#30103"
msgid "Login rate limit reach!"
msgstr "Zu viele Anmeldeversuche!"

#. Notification message to show on screen if server is not reachable
#: src/Session.cpp
msgctxt "#30104"
msgid "Server not reachable!"
msgstr "Server nicht erreichbar!"

#. Notification message to show on screen if server is connected
#: src/Session.cpp
msgctxt "#30105"
msgid "Connected"
msgstr "Verbunden"
